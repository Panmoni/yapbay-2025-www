  "editor.tokenColorCustomizations": {
    "comments": "#718096",
    "strings": "#38b2ac",
    "keywords": "#3182ce",
    "numbers": "#ed8936",
    "types": "#2c5282",
    "functions": "#2b6cb0",
    "variables": "#1a202c",
    "textMateRules": [
      {
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#3182ce"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#38b2ac"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "foreground": "#1a365d",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.quote",
        "settings": {
          "foreground": "#38b2ac",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.inline.raw",
        "settings": {
          "foreground": "#48bb78"
        }
      },
      {
        "scope": "markup.fenced_code.block",
        "settings": {
          "foreground": "#4a5568"
        }
      }
    ]
